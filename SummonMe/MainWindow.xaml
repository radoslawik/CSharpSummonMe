<Window x:Class="SummonMe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SummonMe"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
  
    <Grid>
        <TextBox Text="{Binding SummonerName}" Margin="15" />
        <TextBox Text="{Binding Region}" Margin="15" />
        <TextBlock Text="{Binding Path=Level}" FontSize="80"/>
        <TextBlock Text="{Binding Path=SummonerName}"/>
        <Button x:Name="Button" Margin="15 50" Content="SHOW DATA" Click="Button_Click"/>
    </Grid>
</Window>
